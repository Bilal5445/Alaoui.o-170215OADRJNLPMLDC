@model List<ScrapyWeb.Models.TwitterApplication>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div>
   @Html.ActionLink("Add Application", "AddApplication", "AccountPanel")
    <h2>Twitter Applications</h2>
    <div>
        <table class="users">
            <thead>
                <tr>
                    <th class="row-1 row-ID">App Name</th>
                    <th class="row-2 row-name">Consumer Key</th>
                    <th class="row-3 row-job">Consumer Secret</th>
                    <th class="row-4 row-email">Access Token</th>
                    <th class="row-4 row-email">Token Secret</th>
                </tr>
            </thead>
            <tbody>

                @foreach (var app in Model)
                {
                    <tr>
                        <td>
                            @app.ApplicationName
                        </td>
                        <td>@app.ConsumerKey</td>
                        <td>@app.ConsumerSecret</td>
                        <td>@app.AccessToken</td>
                        <td>@app.AccessTokenSecret</td>
                    </tr>
                }

            </tbody>

        </table>

     

    </div>

    </div>